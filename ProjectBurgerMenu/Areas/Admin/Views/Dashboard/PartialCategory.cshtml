@model List<ProjectBurgerMenu.Entities.Category>
<div class="col-xl-4 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title mb-3">Kategorilerimiz</h4>
            <div class="row">
                <div class="col-sm-12">
                    <div class="text-dark">
                        @foreach (var item in Model)
                        {


                       <div class="d-flex pb-3 border-bottom justify-content-between">
                           <div class="mr-3"><i class="mdi mdi-signal-cellular-outline icon-md"></i></div>
                           <div class="font-weight-bold mr-sm-4">
                               <div>Aktif Ürün Bulunan</div>
                           </div>
                           <div><h6 class="font-weight-bold text-info ml-sm-2">@item.CategoryName</h6></div>

                       </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
