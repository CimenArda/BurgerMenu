@model List<ProjectBurgerMenu.Entities.Address>

@foreach (var item in Model)
{


<div class="col-md-4 site-animate">
    <p><img src="~/eatwell-master/images/about_img_1.jpg" alt="" class="img-fluid"></p>
    <p class="text-black">
        Adres: <br> @item.Location <br> <br>
        Telefon: <br> @item.Phone<br> <br>
        Mail: <br> <a href="mailto:info@yoursite.com">@item.Email</a>
    </p>

</div>
}
